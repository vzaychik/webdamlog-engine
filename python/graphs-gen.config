[query-5]
type = line
name = numfacts_jou_all_space_10_2_1
xlabel = Number of facts per follower, K
ylabel = Total space for all peer tables, MB
sql = select round(J.numFacts/1000,0) as NumFacts, sum(if(J.policy="KNOWN" and J.mode=1,J.sumC2,NULL))/1000000 as "Known", sum(if(J.policy="PUB" and J.mode=1, J.sumC2,NULL))/1000000 as "Public", sum(if(J.policy="KNOWN" and J.mode=7, J.sumC2,NULL))/1000000 as "Known optim (1&2)", sum(if(J.policy="PUB" and J.mode=3, J.sumC2, NULL))/1000000 as "Public optim 1", sum(if(J.policy="PUB" and J.mode=0, J.sumC2,NULL))/1000000 as "No access control" from (Select F.numFacts as numFacts, F.scenario as scenario, F.policy as policy, F.mode as mode, F.EID as ID, F.PNAME, SUM(F.C2) as sumC2, F.C5 as sumC5, F.numFollower as numFollower, F.numAgg as numAgg, F.APF as APF FROM ( Select s.numFacts, s.policy as policy, s.ruleScenario as scenario, e.mode as mode, s.numFollowers as numFollower, s.numAggregators as numAgg, s.aggPerFollower as APF, t.execID_id as EID, t.peerName as PNAME, max(t.tickCount2) as C2, SUM(t.tickCount5) as C5 FROM scenario as s, tick as t, execution as e WHERE t.execID_id = e.execID AND s.scenID = e.scenID_id AND e.build = 15 and s.ruleScenario="JOIN_OF_UNIONS" and s.aggPerFollower=1 and s.numFollowers=10 GROUP BY s.numFacts, s.ruleScenario, s.policy, t.execID_id, t.peerName ) as F GROUP BY F.numFacts, F.scenario, F.policy, F.mode, F.EID, F.PNAME, F.numFollower, F.numAgg, F.APF ) as J GROUP BY J.numFacts order by J.numFacts;

